{
  "name": "depa-web-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "backstage-modal": "^0.3.2",
    "classnames": "^2.2.6",
    "draft-js": "^0.10.5",
    "immutable": "^4.0.0-rc.12",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build-css": "node-sass-chokidar src/styles/easel.sass -o dist/css/ --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/easel.sass -o dist/css/ -w",
    "build:lib": "babel src/ -d lib/",
    "eject": "react-scripts eject",
    "test": "react-scripts test --env=jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
